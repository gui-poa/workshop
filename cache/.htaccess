<IfModule mod_headers.c>
 <FilesMatch "\.(gif|jpg|png)$">
  #Header add "Cache-Control" "no-store"
 </FilesMatch>

<FilesMatch "\.(json|xml)$">
  Header add "Cache-Control" "max-age=20"
 </FilesMatch>
</IfModule>

#Desabilitar os cabecalhos de validacao
#Header unset Etag
#FileETag none
#Header unset Last-Modified